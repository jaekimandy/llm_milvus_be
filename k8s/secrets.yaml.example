apiVersion: v1
kind: Secret
metadata:
  name: gaia-abiz-secrets
  namespace: gaia-abiz
type: Opaque
stringData:
  database-url: "postgresql://user:password@postgres-service:5432/gaia_abiz"
  secret-key: "your-secret-key-change-this-in-production"
  encryption-key: "your-encryption-key-32-bytes-minimum-length"
  openai-api-key: "sk-your-openai-api-key"
  oauth2-client-id: "your-oauth2-client-id"
  oauth2-client-secret: "your-oauth2-client-secret"
  oauth2-redirect-uri: "https://your-domain.com/auth/callback"
